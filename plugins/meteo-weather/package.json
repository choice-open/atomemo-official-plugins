{
  "name": "meteo-weather",
  "type": "module",
  "version": "0.2.4",
  "description": "open-meteo-weather",
  "author": "leo.yu <coolfish2004@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/choice-open/atomemo-official-plugins/plugins/meteo-weather#readme",
  "repository": "https://github.com/choice-open/atomemo-official-plugins/plugins/meteo-weather",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check . --write",
    "dev": "concurrently -c=cyan.bold,blue.bold --names=DEV,I18N --pad-prefix \"tsdown --watch\" \"typesafe-i18n\"",
    "test": "vitest",
    "release": "bumpp package.json definition.json --commit 'release meteo-weather v%s' --no-push --no-tag --yes",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@choiceopen/atomemo-plugin-sdk-js": "latest",
    "dotenv": "^17.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/bun": "^1.3.9",
    "bumpp": "^10.4.1",
    "concurrently": "9.2.1",
    "tsdown": "^0.20.3",
    "typesafe-i18n": "^5.27.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  }
}
