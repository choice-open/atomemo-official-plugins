{
  "name": "deepseek",
  "type": "module",
  "version": "0.1.1",
  "description": "DeepSeek provides advanced AI capabilities for chats and completions. This plugin enables developers to integrate DeepSeek's models, including text generation (deepseek-chat and deepseek-code) via the API.",
  "author": "david 2 <david.chen@choiceform.com>",
  "license": "MIT",
  "homepage": "https://github.com/choice-open/atomemo-official-plugins/plugins/deepseek#readme",
  "repository": "https://github.com/choice-open/atomemo-official-plugins/plugins/deepseek",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check . --write",
    "dev": "concurrently -c=cyan.bold,blue.bold --names=DEV,I18N --pad-prefix \"tsdown --watch\" \"typesafe-i18n\"",
    "test": "vitest",
    "release": "bumpp package.json definition.json --commit 'release deepseek v%s' --no-push --no-tag --yes",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@choiceopen/atomemo-plugin-sdk-js": "latest",
    "dotenv": "^17.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/bun": "^1.3.9",
    "bumpp": "^10.4.1",
    "concurrently": "9.2.1",
    "tsdown": "^0.20.3",
    "typesafe-i18n": "^5.27.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  }
}
