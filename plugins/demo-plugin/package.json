{
  "name": "demo-plugin",
  "type": "module",
  "version": "0.5.0",
  "description": "A brief description of the plugin's functionality",
  "author": "余凡 <dev.yufan@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/choice-open/atomemo-official-plugins/plugins/demo-plugin#readme",
  "repository": "https://github.com/choice-open/atomemo-official-plugins/plugins/demo-plugin",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check . --write",
    "dev": "concurrently -c=cyan.bold,blue.bold --names=DEV,I18N --pad-prefix \"tsdown --watch\" \"typesafe-i18n\"",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "preversion": "bun run check && bun run build --clean",
    "version": "bumpp package.json definition.json --commit 'v%s' --tag 'v%s'",
    "postversion": "echo Updated to version $(node -p \"require('./package.json').version\")"
  },
  "dependencies": {
    "@choiceopen/atomemo-plugin-sdk-js": "latest",
    "dotenv": "^17.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "^1.3.8",
    "bumpp": "^10.4.1",
    "concurrently": "9.2.1",
    "tsdown": "^0.20.3",
    "typesafe-i18n": "^5.26.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  }
}
