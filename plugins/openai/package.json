{
  "name": "openai",
  "type": "module",
  "version": "0.3.3",
  "description": "OpenAI offers a comprehensive set of models for various tasks, including text generation, image generation, vision, audio generation, text-to-speech (TTS), speech-to-text (STT), embeddings, moderation, and reasoning.",
  "author": "david 2 <david.chen@choiceform.com>",
  "license": "MIT",
  "homepage": "https://github.com/choice-open/atomemo-official-plugins/plugins/openai#readme",
  "repository": "https://github.com/choice-open/atomemo-official-plugins/plugins/openai",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check . --write",
    "dev": "concurrently -c=cyan.bold,blue.bold --names=DEV,I18N --pad-prefix \"tsdown --watch\" \"typesafe-i18n\"",
    "test": "vitest",
    "release": "bumpp package.json definition.json --commit 'release openai v%s' --no-push --no-tag --yes",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@choiceopen/atomemo-plugin-sdk-js": "latest",
    "dotenv": "^17.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "^1.3.9",
    "bumpp": "^10.4.1",
    "concurrently": "9.2.1",
    "tsdown": "^0.20.3",
    "typesafe-i18n": "^5.27.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  }
}
